<%= simple_form_for @company do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zip %>
  <%= f.input :phone %>
  <%= f.input :fax %>
  <%= f.button :submit, class: "btn btn-default" %>
<% end %>
<%= link_to 'Cancel', root_path, class: "btn btn-default" %>